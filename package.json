{
  "name": "phin-tools",
  "displayName": "phin-tools",
  "description": "A simple set of macros and tools i like to use",
  "publisher": "phin-tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/phin-tech/phin-tools"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.clipboardToFilePath",
        "title": "Clipboard to File Path: Write clipboard to file"
      }
    ],
    "keybindings": [
      {
        "command": "extension.clipboardToFilePath",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "package-extension": "vsce package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.x",
    "eslint": "^8.26.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "vscode-test": "^1.6.1",
    "ts-loader": "^9.4.2",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  }
}